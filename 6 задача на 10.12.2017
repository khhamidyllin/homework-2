\\Напишите программу выводящую все несократимые дроби с неотрицательными знаменателем и числителем не превышающим N, в порядке возрастания. Будем считать дробь 1/0 - максимальной несократимой.
#include <iostream>
using namespace std;
int main()
{
int m,n,b,s;
cin>>b;
s=0;
m=0;
while (n<b)
{
    m=m+1;
    n=n+1;
    if ((m%n)!=0)
        {cout<<m<<"/"<<n<<" ";}

}
}
